package ec.com.bancoInternacional.view.domain;

import java.io.Serializable;
import java.math.BigDecimal;

@SuppressWarnings("serial")
public class AccionistasDto  implements Serializable {
	private static final long serialVersionUID = -6240186599471971974L;
	
	private Integer countAccionista;
	private String tipoNatural;
	private String tipoJuridico;
	private String tipo;
	private String identificacion;
	private String nombre;
	private BigDecimal porcentaje;
	private BigDecimal monto;
	private String nacionalidad;
	private String pepSi;
	private String pepNo;
	private String pep;
	
	public AccionistasDto() {
		super();
	}
	public String getTipoNatural() {
		return tipoNatural;
	}
	public void setTipoNatural(String tipoNatural) {
		this.tipoNatural = tipoNatural;
	}
	public String getTipoJuridico() {
		return tipoJuridico;
	}
	public void setTipoJuridico(String tipoJuridico) {
		this.tipoJuridico = tipoJuridico;
	}
	public String getIdentificacion() {
		return identificacion;
	}
	public void setIdentificacion(String identificacion) {
		this.identificacion = identificacion;
	}
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	public BigDecimal getPorcentaje() {
		return porcentaje;
	}
	public void setPorcentaje(BigDecimal porcentaje) {
		this.porcentaje = porcentaje;
	}
	public BigDecimal getMonto() {
		return monto;
	}
	public void setMonto(BigDecimal monto) {
		this.monto = monto;
	}
	public String getNacionalidad() {
		return nacionalidad;
	}
	public void setNacionalidad(String nacionalidad) {
		this.nacionalidad = nacionalidad;
	}
	public String getPepSi() {
		return pepSi;
	}
	public void setPepSi(String pepSi) {
		this.pepSi = pepSi;
	}
	public String getPepNo() {
		return pepNo;
	}
	public void setPepNo(String pepNo) {
		this.pepNo = pepNo;
	}
	public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	public Integer getCountAccionista() {
		return countAccionista;
	}
	public void setCountAccionista(Integer countAccionista) {
		this.countAccionista = countAccionista;
	}
	public String getPep() {
		return pep;
	}
	public void setPep(String pep) {
		this.pep = pep;
	}
	
	

}

				<text><![CDATA[   4.	DECLARACIONES:]]></text>
			</staticText>
			<textField isStretchWithOverflow="true">
				<reportElement x="0" y="20" width="555" height="23" uuid="e37f5155-ab79-45fa-9fdb-2035fd259519">
